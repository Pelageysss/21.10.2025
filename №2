#include <iostream>
#include <string>
#include <vector>
using namespace std;
int main() {
    int n,arrs;
    cout<<"Enter the array size (a whole natural number) : ";
    cin>>n;
    int arr[n]; 
    for (int i = 0; i < n; i++) {
        cout<<"Enter "<< i+1 <<" an array element : ";
        cin>>arr[i];
    }
    for (int i = 0; i < n; i++) {
        if ((arr[i]%2)==0){
            swap(arr[i],arr[i-1]);
        }
    }
    for (int i = 0; i < n; i++) {
        cout<<arr[i]<<" ";
    }
}
