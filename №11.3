#include <iostream>
#include <list>
#include <string>

int main() {
    std::list<std::string> students = {"Иванов", "Петров", "Сидоров", "Алексеев", "Борисов"};
    students.sort();
    students.remove_if([](const std::string& surname) {
        return !surname.empty() && surname[0] == 'А';
    });
    for (const auto& student : students) {
        std::cout << student << std::endl;
    }
    
    return 0;
}
