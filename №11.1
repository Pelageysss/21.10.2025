#include <iostream>
#include <array>
#include <numeric>
#include <iomanip>

int main() {
    std::array<int, 10> arr;
    for (size_t i = 0; i < arr.size(); ++i) {
        arr[i] = i + 1;
    }
    
    double average = static_cast<double>(std::accumulate(arr.begin(), arr.end(), 0)) / arr.size();
    
    std::cout << std::fixed << std::setprecision(2) << average << std::endl;
    
    return 0;
}
