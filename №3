#include <iostream>
#include <string>
#include <vector>
using namespace std;
int main() {
    int n,arrs,j;
    cout<<"Enter the array size (a whole natural number) : ";
    cin>>n;
    int arr[n]; 
    for (int i = 0; i < n; i++) {
        cout<<"Enter "<< i+1 <<" an array element : ";
        cin>>arr[i];
    }
    arrs=0;
    for (int i = 0; i < n; i++) {
        if ((arr[i]%3)==0){
            arrs++;
        }
    }
    int hh[arrs];
    j=0;
     for (int i = 0; i < n; i++) {
        if ((arr[i]%3)==0){
            hh[j]=arr[i];
            j++;
        }
    }
    for (int i = 0; i < arrs; i++) {
        cout<<hh[i]<<" ";
    }
}
