#include <iostream>
#include <list>

int main() {
    std::list<int> a = {1, 3, 5, 7};
    std::list<int> b = {2, 3, 6, 7, 8};
    a.merge(b);
    a.unique();
    a.remove_if([](int x) { return x > 6; });
    
    for (const auto& elem : a) {
        std::cout << elem << " ";
    }
    
    return 0;
}
